<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.meet.newcalci.Main2Activity">

    <LinearLayout

        android:orientation="vertical"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
            android:id="@+id/btnmain2imgdis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Live Image Demo"/>



        <Button
            android:id="@+id/btnmain2calc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Calculator"/>

        <Button
        android:id="@+id/btnmain2login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"/>

        <Button
            android:id="@+id/btnmain2signup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SignUp"/>


        <Button
            android:id="@+id/btnmain2data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Live WebService Demo"/>



    </LinearLayout>

    <LinearLayout
        android:paddingTop="100dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    </LinearLayout>

</LinearLayout>
